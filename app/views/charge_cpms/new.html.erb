<h1>New charge_cpm</h1>

<% form_for(@charge_cpm) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :unit_price %><br />
    <%= f.text_field :unit_price %>
  </p>
  <p>
    <%= f.label :discount %><br />
    <%= f.text_field :discount %>
  </p>
  <p>
    <%= f.label :total_budget %><br />
    <%= f.text_field :total_budget %>
  </p>
  <p>
    <%= f.label :priority %><br />
    <%= f.text_field :priority %>
  </p>
  <p>
    <%= f.label :daily_threshold %><br />
    <%= f.text_field :daily_threshold %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', charge_cpms_path %>