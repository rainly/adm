<h1>New charge_cpt</h1>

<% form_for(@charge_cpt) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :unit_price %><br />
    <%= f.text_field :unit_price %>
  </p>
  <p>
    <%= f.label :discount %><br />
    <%= f.text_field :discount %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', charge_cpts_path %>