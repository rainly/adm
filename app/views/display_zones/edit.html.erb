<h1>Editing display_zone</h1>

<% form_for(@display_zone) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :campaign_id %><br />
    <%= f.text_field :campaign_id %>
  </p>
  <p>
    <%= f.label :ad_zone_id %><br />
    <%= f.text_field :ad_zone_id %>
  </p>
  <p>
    <%= f.label :expression %><br />
    <%= f.text_field :expression %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :owner_id %><br />
    <%= f.text_field :owner_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @display_zone %> |
<%= link_to 'Back', display_zones_path %>