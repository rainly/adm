<h1>Listing ad_zone_categories</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Owner</th>
  </tr>

<% @ad_zone_categories.each do |ad_zone_category| %>
  <tr>
    <td><%=h ad_zone_category.name %></td>
    <td><%=h ad_zone_category.owner_id %></td>
    <td><%= link_to 'Show', ad_zone_category %></td>
    <td><%= link_to 'Edit', edit_ad_zone_category_path(ad_zone_category) %></td>
    <td><%= link_to 'Destroy', ad_zone_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New ad_zone_category', new_ad_zone_category_path %>