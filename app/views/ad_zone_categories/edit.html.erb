<h1>Editing ad_zone_category</h1>

<% form_for(@ad_zone_category) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :owner_id %><br />
    <%= f.text_field :owner_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @ad_zone_category %> |
<%= link_to 'Back', ad_zone_categories_path %>